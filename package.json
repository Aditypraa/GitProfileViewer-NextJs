{
  "name": "git-profile-viewer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "tsc": "tsc --noEmit",
    "format-write": "prettier --write \"**/*.{ts,tsx,js,json}\"",
    "format-check": "prettier --check \"**/*.{ts,tsx,js,json}\"",
    "check": "npm run format-write && npm run format-check && npm run lint && npm run tsc",
    "prepare": "husky"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "next": "^15.3.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "rehype-raw": "^7.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.8",
    "husky": "^9.1.7",
    "lint-staged": "^16.0.0",
    "prettier": "3.5.3",
    "typescript": "^5"
  }
}
